/*--------[Custom Skins Models]--------*/
forward GetLauncherSkinModel(current_model);
public GetLauncherSkinModel(current_model)
{
    new model = -1;
    switch(current_model)
    {
        case 14910: model = 154;
    }
    return model;
}
//
/*--------[Custom Vehicles Models]--------*/
forward GetLauncherVehicleModel(current_model);
public GetLauncherVehicleModel(current_model)
{
	new model = GetNewVehicle(current_model);
	return model;
}
stock GetNewVehicle(model)
{
	for(new i; i < sizeof(NewCaro); i++)
	{
	    if(model != NewCaro[i][NewCarLauncher]) continue;
	    return NewCaro[i][NewCarClient];
	}
	return -1;
}
