stock KVMoney()
{
    ClearTotalGz();
	for(new i = 0; i < 20; i++) FrakCD[i] = 0;
	for(new i = 0; i < sizeof(GZInfo); i++)
	{
		if(GZInfo[i][gFrakVlad] == 6) TotalGz[0]++;
		if(GZInfo[i][gFrakVlad] == 7) TotalGz[1]++;
		if(GZInfo[i][gFrakVlad] == 8) TotalGz[2]++;
		if(GZInfo[i][gFrakVlad] == 9) TotalGz[3]++;
		if(GZInfo[i][gFrakVlad] == 10) TotalGz[4]++;
	}
	BallasBank += TotalGz[0]*150;
	VagosBank += TotalGz[1]*150;
	GroveBank += TotalGz[2]*150;
	AztecaBank += TotalGz[3]*150;
	RifaBank += TotalGz[4]*150;
}
