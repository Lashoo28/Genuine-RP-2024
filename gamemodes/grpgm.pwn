			SetPlayerFacingAngle(playerid, BizzInfo[b][bEnterPosC]);
			SetPlayerVirtualWorld(playerid, 0);
			SetPlayerInterior(playerid, 0);
			return true;
		}
	}
	return true;
}
public OnPlayerSelectedMenuRow(playerid, row)
{
	if(GetPlayerMenu(playerid) == ReconMenu)
	{
	    if(PI[playerid][pAdmin] == 0) return errortxt(playerid, "Tkven Ar Gakvt Am Funkciis Gamoyenebis Ufleba!");
	    switch(row)
	    {
	        case 0: ShowStats(playerid, SpectateID[playerid]), SpectateON(playerid, SpectateID[playerid]);
	        case 1:
	        {
	            new ip[15], ip_str[150];
				GetPlayerIp(SpectateID[playerid], ip, 15);
				format(ip_str, sizeof(ip_str), "Saxeli: [%s] Registraciis IP [%s] - Bolo Avtorizaciis IP [%s]", Name(SpectateID[playerid]), PI[SpectateID[playerid]][pRegIp], ip);
