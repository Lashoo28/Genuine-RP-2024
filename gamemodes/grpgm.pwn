fpublic CreateFamily(playerid, familyname[])
{
	new rows;
	cache_get_row_count(rows);
	if(rows) return errortxt(playerid, "Tkvens Mier Sheyvanili Saxeli Ukve Gamoyenebulia!"), ShowPlayerDialog(playerid, 197, DIALOG_STYLE_INPUT, "{3399FF}Create Family{FFFFFF} | Miutitet Saxeli", "{FFFFFF}Family's Shesakmnelad Sachiroa Miutitot Saxeli\nFamily's Shekmnis Safasuri: {3399FF}50.000$\n\tCifrebis Gamoyeneba Daushvebelia\n\tSaxeli Unda Shedgebodes Minimum 4 Da Maxsimum 10 Simbolosgan", "Shemdeg", "Daxurva");
	else if(!rows)
	{
		if(PI[playerid][pFamilyMember] != 999) return errortxt(playerid, "Tkven Ukve Imyofebit Family'shi!");
		if(PI[playerid][pMoney] < 150000) return errortxt(playerid, "Tkven Ar Gakvt Sakmarisi Raodenobis Tanxa, Sachiroa: 150000.000$");
	}
	TotalFamily += 1;
	if(TotalFamily >= MAX_FAMILY) return errortxt(playerid, "Serverze Ukve Maxsimaluri Raodenobit Sheikmna Family!"), TotalFamily -= 1;
	PI[playerid][pMoney] -= 150000;
	UpdatePlayerData(playerid, "Money", PI[playerid][pMoney]);
	GameTextForPlayer(playerid, "~r~-150.000$", 1000, 1);
	PI[playerid][pFamilyMember] = TotalFamily;
	UpdatePlayerData(playerid, "FamilyMember", PI[playerid][pFamilyMember]);
	PI[playerid][pFamilyRank] = 10;
	UpdatePlayerData(playerid, "FamilyRank", PI[playerid][pFamilyRank]);
	format(FamilyInfo[PI[playerid][pFamilyMember]][fName], 25, familyname);
	format(FamilyInfo[PI[playerid][pFamilyMember]][fLeader], MAX_PLAYER_NAME, Name(playerid));
	format(FamilyInfo[PI[playerid][pFamilyMember]][fColor], 10, "FFAE00");
	FamilyInfo[PI[playerid][pFamilyMember]][fType] = 0;
	format(log_str, sizeof(log_str), "%s'm(a) Gaaketa Family: %s Da Gadaixada: 50.000$", Name(playerid), familyname);
	CreateLog("Money", log_str);
	new family_str[280];
	mysql_format(connect, family_str, sizeof(family_str), "INSERT INTO `Family` (`Name`, `Leader`, `Color`, `Type`) VALUES ('%e', '%e', 'FFAE00', '1')", familyname, Name(playerid));
	mysql_query(connect, family_str);
	return true;
}
