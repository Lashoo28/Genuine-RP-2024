CMD:report(playerid, params[])
{
	if(sscanf(params, "ds[256]", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /report [Kitxva: 0 / Sachivari: 1] [Text]");
	if(strlen(params[1]) > 60) return errortxt(playerid, "Tkvens Mier Sheyvanili Text Dzalian Didia!");
	if(ReportID[playerid] != 25) return errortxt(playerid, "Tkveni Shekitxva/Sachivari Ukve Gagzavnilia, Daelodet Administracias!");
	if(GetPVarInt(playerid, "SendReportTime") > gettime()) return errortxt(playerid, "Reportis Gamoyeneba Shesadzlebelia Yovel 2Wutshi Ertxel!");
	if(TotalReport == MAX_REPORTS) return errortxt(playerid, "Sachivrebis Sia Gadatvirtulia, Scadet Ramodenime Wutshi!");
	if(params[0] < 0 || params[0] > 1) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /report [Shekitxva: 0 / Sachivari: 1] [Text]");
	if(PI[playerid][pMute] == 1) return errortxt(playerid, "Tkven Gadevt Chat'is Bani!");
	if(strlen(params[0]) > 60) return errortxt(playerid, "Tkvens Mier Sheyvanili Text Dzalian Didia!");
	new reports = 1, str[32];
	for(new i; i < MAX_REPORTS; i++)
	{
	    if(ReportInfo[i][rStatus] == true) reports ++;
		if(ReportInfo[i][rStatus] == false)
		{
			format(other_string, sizeof(other_string), "{3399FF}[REPORT] {FFFFFF}Tkven Gaagzavnet: {3399FF}%s", params[1]);
			SendClientMessage(playerid, COLOR_WHITE, other_string);
			ReportInfo[i][rStatus] = true;
			strmid(ReportInfo[i][rName], Name(playerid), 0, strlen(Name(playerid)), 64);
			ReportInfo[i][rID] = playerid;
			strmid(ReportInfo[i][rText], params[1], 0, strlen(params[1]), 64);
			if(params[0] == 0) ReportInfo[i][rReason] = false, format(other_string, sizeof(other_string), "{3399FF}[A] %s[%d]{FFFFFF}'m(a) Gamoagzavna: {3399FF}Shekitxva{FFFFFF}, Daachiret: {3399FF}H [%d]", Name(playerid), playerid, TotalReport);
			if(params[0] == 1) ReportInfo[i][rReason] = true, format(other_string, sizeof(other_string), "{3399FF}[A] %s[%d]{FFFFFF}'m(a) Gamoagzavna: {3399FF}Sachivari{FFFFFF}, Daachiret: {3399FF}H [%d]", Name(playerid), playerid, TotalReport);
			ReportInfo[i][rAnswer] = false;
			ReportID[playerid] = i;
			TotalReport += 1;
			SetPVarInt(playerid, "SendReportTime", gettime()+120);
			SendAdminMessage(COLOR_WHITE, other_string);
			break;
		}
	}
	format(str, sizeof(str), "%i", reports);
	TextDrawSetString(ainfo_TD[3], str);
	return true;
}
