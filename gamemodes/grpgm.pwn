stock SpectateON(playerid, specid)
{
	SetPVarInt(playerid, "ReconSkin", GetPlayerSkin(playerid));
	if(AdminRecon[playerid] == false)
	{
		GetPlayerPos(playerid, AdminPos[playerid][0], AdminPos[playerid][1], AdminPos[playerid][2]);
		GetPlayerFacingAngle(playerid, AdminPos[playerid][3]);
		AdminVirt[playerid] = GetPlayerVirtualWorld(playerid);
		AdminInt[playerid] = GetPlayerInterior(playerid);
		AdminRecon[playerid] = true;
	}
	if(IsPlayerInAnyVehicle(specid))
	{
		SetPlayerInterior(playerid,GetPlayerInterior(specid));
		SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(specid));
		TogglePlayerSpectating(playerid, true);
		PlayerSpectateVehicle(playerid, GetPlayerVehicleID(specid));
		SpectateID[playerid] = specid;
	}
	else
	{
		SetPlayerInterior(playerid,GetPlayerInterior(specid));
		SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(specid));
		TogglePlayerSpectating(playerid, true);
		PlayerSpectatePlayer(playerid, specid);
		SpectateID[playerid] = specid;
	}
	ShowMenuForPlayer(ReconMenu,playerid);
    SpectateStatus[playerid] = true;
	return true;
}
stock SpectateOFF(playerid)
{
    SetPlayerSkin(playerid, 0);
    //SpawnPlayer(playerid);
	SetPVarInt(playerid, "ReconSkin", 0);
    HideMenuForPlayer(ReconMenu, playerid);
	TogglePlayerSpectating(playerid, false);
	SpectateID[playerid] = INVALID_PLAYER_ID;
	GameTextForPlayer(playerid, "~w~RECON ~r~ OFF",5000,3);
    SpectateStatus[playerid] = false;
	if(AdminDuty[playerid] == true)
	{
		SetPlayerColor(playerid, 0xFFFFFFAA);
		SetPlayerWeapon(playerid, 38, 99999);
	}
	return true;
}
