	new roadblockid = -1;
	for(new i = 0; i < MAX_ROADBLOCKS; i ++)
	{
	    if(RoadBlockData[i][rb_Status]) continue;
		roadblockid = i;
		break;
	}
	if(roadblockid == -1) return false;
	RoadBlockData[roadblockid][rb_Status] = true;
	RoadBlockData[roadblockid][rb_Object] = CreateDynamicObject(3091, x, y, z, 0, 0, r);
    RoadBlockData[roadblockid][rb_Pos][0] = x;
    RoadBlockData[roadblockid][rb_Pos][1] = y;
    RoadBlockData[roadblockid][rb_Pos][2] = z;
    RoadBlockData[roadblockid][rb_Time] = time;
    return true;
}
