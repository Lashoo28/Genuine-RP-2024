    {
        PI[playerid][pMagida] = 5;
	}
	if(stolid == 6 && KazinoDuty[playerid] == 1)
    {
        PI[playerid][pMagida] = 7;
	}
	if(stolid == 7 && KazinoDuty[playerid] == 1)
    {
        PI[playerid][pMagida] = 8;
	}
