public OnPlayerDeath(playerid, killerid, reason)
{
	SetPlayerArmour(playerid, 0);
	FixLauncherSkin(playerid, 2990);
	foreach(new i:Player)
	{
        if(PI[i][pAdmin] >= 1 && Alogin[i] == true) SendDeathMessageToPlayer(i, killerid, playerid, reason);
    }
	for(new i = 0; i<sizeof(GZInfo); i++)
	{
		if(ZoneOnBattle[i] == 1 && killerid != INVALID_PLAYER_ID)
		{
			if(PI[killerid][pMember] == GZInfo[i][gNapad] && PI[killerid][pMember] != PI[playerid][pMember] ||
		    PI[killerid][pMember] == GZInfo[i][gFrakVlad] && PI[killerid][pMember] != PI[playerid][pMember])
		    {
				if(PI[playerid][pMember] == GZInfo[i][gNapad] || PI[playerid][pMember] == GZInfo[i][gFrakVlad])
   				{
   					foreach(new n:Player)
			 		{
			 		    if(PI[n][pMember] == GZInfo[i][gNapad] || PI[n][pMember] == GZInfo[i][gFrakVlad]) SendDeathMessageToPlayer(n, killerid, playerid, reason);
					}
   				    CountOnZone[PI[killerid][pMember]] += 1;
   				    KillCounter[killerid] += 1;
   				    KillName[killerid] += 1;
   				    KillName[playerid] = 0;
   				    PI[killerid][pKills] += 1;
   				    PI[playerid][pDeaths] += 1;
   				    PlayerInformation(killerid);
   				    PlayerInformation(playerid);
				 	SetPlayerToTeamColor(killerid);
				 	GhettoKillNames(killerid);
				}
			}
		}
	}
	new string[128];
	for(new i =0;i<sizeof(McInfo);i++)
	{
		if(ZoneOnMBattle[i] == 1 && killerid != INVALID_PLAYER_ID)
		{
		    if(PI[killerid][pMember] == McInfo[i][Napad] && PI[killerid][pMember] != PI[playerid][pMember] ||
		    PI[killerid][pMember] == McInfo[i][FrakVlad] && PI[killerid][pMember] != PI[playerid][pMember])
		    {
				if(PI[playerid][pMember] == McInfo[i][Napad] || PI[playerid][pMember] == McInfo[i][FrakVlad])
   				{
   				    if(PlayerToKvadrat(playerid,McInfo[i][Coords][0],McInfo[i][Coords][1],McInfo[i][Coords][2],McInfo[i][Coords][3]) && PlayerToKvadrat(killerid,McInfo[i][Coords][0],McInfo[i][Coords][1],McInfo[i][Coords][2],McInfo[i][Coords][3]))
					{
						CountOnMFZone[PI[killerid][pMember]] += 1;
					 	MKillCounter[killerid] += 1;
					 	moklaWarze[killerid] += 1;
					 	moklaWarze[playerid] = 0;
					 	KillNames[killerid] += 1;
						KillNames[playerid] = 0;

						PI[killerid][pKills] += 1;
                        UpdatePlayerData(killerid, "Kills", PI[killerid][pKills]);
                        PI[playerid][pDeaths] += 1;
                        UpdatePlayerData(playerid, "Deaths", PI[playerid][pDeaths]);
                        GhettoKillNames(killerid);
					 	if(moklaWarze[killerid] == 5)
						{
							format(string,sizeof(string),"{FF0000}[MaffiaInfo] {FFFFFF}%s'm(a) Gaaketa -5Kill | Gadaeca: +10.000$", Name(killerid));
							PI[killerid][pMoney] += 10000;
							UpdatePlayerData(killerid, "Money", PI[killerid][pMoney]);
							moklaWarze[killerid] = 0;
							SendFamilyMessage(McInfo[i][FrakVlad],COLOR_WHITE,string);
							SendFamilyMessage(McInfo[i][Napad],COLOR_WHITE,string);
				        }
				 		foreach(new n:Player)
				 		{
				 		    if(PI[n][pMember] == McInfo[i][Napad] || PI[n][pMember] == McInfo[i][FrakVlad])
				 		    {
				 				SendDeathMessageToPlayer(n, killerid, playerid, reason);
				 				if(DMArena[playerid] != 0) return true;
							}
						}
					}
				}
			}
		}
	}
	maskson[playerid] = false;
	dubinka[playerid] = false;
	if(animon[playerid] == true)
	{
	 	animon[playerid] = false;
	    TextDrawHideForPlayer(playerid, StopAnimation);
	}
	if(DMArena[playerid] != 0 && DMArena[killerid] != 0)
	{
	    SetPlayerHealth(killerid, 100);
	    KillName[killerid] += 1;
	    KillName[playerid] = 0;
	    GhettoKillNames(killerid);
     	DMKill[killerid] ++;
    	DMDeath[playerid] ++;
    	UpdateDeathMatchText(killerid);
    	UpdateDeathMatchText(playerid);
	}
	if(BankMoney[playerid] == true)
	{
	    KillTimer(BankMoneyTime[playerid]);
	    BankMoney[playerid] = false;
		RemovePlayerAttachedObject(playerid, 1);
	    SetPlayerAttachedObject(killerid, 1, 1550, 1, 0, -0.25, 0, 0, 45, 0);
		BankMoney[killerid] = true;
		BankMoneyTime[killerid] = SetTimerEx("BankMoneyTimer" , 60000, false, "d", playerid);
	}
	if(PI[playerid][pAdmin] <= 0 && MFCaptOn[playerid] == 0 && CaptureOn[playerid] == 0 && DMArena[playerid] == 0 && PlayerEvent[playerid] == false)
	{
	    if(killerid != INVALID_PLAYER_ID && PI[killerid][pAdmin] > 0)
	    {
     		KnockStatus[playerid] = false;
	        if(PI[playerid][pMember] == 0) SpawnInHospital[playerid] = true;
	    }
	    else if(KnockStatus[playerid])
	    {
	        KnockStatus[playerid] = false;
	        if(PI[playerid][pMember] == 0) SpawnInHospital[playerid] = true;
	    }
	    else
	    {
	        SetPlayerHealth(playerid, 120);
	        KnockStatus[playerid] = true;
		    GetPlayerPos(playerid, KnockPos[playerid][0], KnockPos[playerid][1], KnockPos[playerid][2]);
		    GetPlayerFacingAngle(playerid, KnockPos[playerid][3]);
			KnockData[playerid][0] = GetPlayerInterior(playerid);
			KnockData[playerid][1] = GetPlayerVirtualWorld(playerid);
		}
	}
	if(Learn_Fight[playerid] > 0)
	{
		Learn_Fight[playerid] = 0;
		SetPlayerFightingStyle (playerid, FIGHT_STYLE_NORMAL);
	}
	if(PI[playerid][pWanted] > 0 && killerid != INVALID_PLAYER_ID && IsACop(killerid))
	{
	    PI[playerid][pWanted] = 0;
	    UpdatePlayerData(playerid, "Wanted", PI[playerid][pWanted]);
        new jailtype = Random(1,3), jailcell = Random(1,7);
        PI[playerid][pJail] = jailtype;
        UpdatePlayerData(playerid, "Jail", PI[playerid][pJail]);
        PI[playerid][pJailCell] = jailcell;
        UpdatePlayerData(playerid, "JailCell", PI[playerid][pJailCell]);
        PI[playerid][pJailTime] = 1200;
        UpdatePlayerData(playerid, "JailTime", PI[playerid][pJailTime]);
        PI[playerid][pSpawnType] = 3;
        UpdatePlayerData(playerid, "SpawnType", PI[playerid][pSpawnType]);
	}
	return true;
}
