			if(StolInfo[GetPVarInt(playerid,"GameStol")][Igrokov] >= MIN_IGROKOV)
			{
			    foreach(new i:Player)
				{
					if(GetPVarInt(i,"StolReg") == GetPVarInt(playerid,"GameStol")) test2++;
				}
				if(test2 < MIN_IGROKOV) return errortxt(playerid, "Motamasheebs Ar Gauketebiat Fsoni!");
   				if(StolInfo[GetPVarInt(playerid,"GameStol")][GameStart] == 1) return errortxt(playerid, "Tamashi Ukve Dawyebulia!");
				TimeStavka[GetPVarInt(playerid,"GameStol")] = 30; StolInfo[GetPVarInt(playerid,"GameStol")][GameStart] = 1; UpdateCasino2();
			}
			else SendClientMessage(playerid,COLOR_GREY,"Ararian Sakmarisad Monawileebi");
			return true;
		}
  		else
		{
		    if(GetPVarInt(playerid,"GameStol") != GetPVarInt(playerid,"StolReg")) return errortxt(playerid, "Tkven Jer Ar Gagiketebiat Fsoni!");
		    if(StolInfo[GetPVarInt(playerid,"GameStol")][GameStart] == 0) return errortxt(playerid, "Am Momentshi Ver Gaagorebt Qvebs!");
	  		if(GetPVarInt(playerid,"Ochki") != 88) return errortxt(playerid, "Tkven Ukve Gaagoret Qvebi!");
			if(OchkiDice[playerid] == 0)
  			{
				new vip5 = Random(2,12);
				SetPVarInt(playerid,"Ochki",vip5);
			}
      		else if(OchkiDice[playerid] == 1)
			{
				new vip = Random(9,12);
				SetPVarInt(playerid,"Ochki",vip);
			}
			else if(OchkiDice[playerid] == 2)
			{
				SetPVarInt(playerid,"Ochki", GetPVarInt(playerid,"OchkiDicedice"));
			}
   			else if(OchkiDice[playerid] == 3)
			{
				SetPVarInt(playerid,"Ochki", GetPVarInt(playerid,"NewOchkiDiceDice"));
				OchkiDice[playerid] = 0;
				DeletePVar(playerid, "NewOchkiDiceDice");
			}
			new test = 0;
			UpdateCasino2();
			for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(GetPVarInt(i,"StolReg") == GetPVarInt(playerid,"StolReg") && GetPVarInt(i,"Ochki") == 88 && KazinoDuty[i] == 0) test++;
			}
			if(test == 0) TimeStavka[GetPVarInt(playerid,"StolReg")] = 5;
		}
		SetPVarInt(playerid, "Stavka", 0);
	}
	return true;
}
