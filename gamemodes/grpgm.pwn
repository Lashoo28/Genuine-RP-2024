stock SaveGZ()
{
	new savegz[400];
	new File = ini_openFile("gamefiles/gangzone.ini");
	new l[10];
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		format(l,sizeof(l), "Zone%d",i);
		format(savegz,sizeof(savegz), "%f|%f|%f|%f|%d",GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3],GZInfo[i][gFrakVlad]);
		ini_setString(File,l,savegz);
	}
	ini_closeFile(File);
}
stock split(const strsrc[], strdest[][], delimiter)
{
	new i, li;
	new aNum;
	new len;
	while(i <= strlen(strsrc))
	{
		if(strsrc[i] == delimiter || i == strlen(strsrc))
		{
			len = strmid(strdest[aNum], strsrc, li, i, 128);
			strdest[aNum][len] = 0;
			li = i+1;
			aNum++;
		}
		i++;
	}
	return true;
}
