public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(newstate == PLAYER_STATE_DRIVER)
	{
	    new newcar = GetPlayerVehicleID(playerid);
	    if(PI[playerid][pLicenses][0] == 0 && PracticeStatus[playerid] == false)
	    {
			if(VehicleLic(newcar))
			{
			    SendClientMessage(playerid, COLOR_WHITE, "{3399FF}Martvis Mowmoba{FFFFFF} | Tkven Ar Flobt Am Transportis Martvis Uflebas!");
			    RemovePlayerFromVehicle(playerid);
			}
	    }
	    if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 481 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 509 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 510)
		{
			GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(GetPlayerVehicleID(playerid),VEHICLE_PARAMS_ON,lights,alarm,doors,bonnet,boot,objective);
		}
		if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 481 && GetVehicleModel(GetPlayerVehicleID(playerid)) != 509 || GetVehicleModel(GetPlayerVehicleID(playerid)) != 510)
		{
			SendClientMessage(playerid, 0x55AB6EAA, "Manqanis Dasaqoqad Gamoikenet :{DFCFCF} /engine & '2'");
			SendClientMessage(playerid, 0x55AB6EAA, "Mankanis Menu's Sanaxavad Gamoikenet :{DFCFCF} /cmenu");
		}
		if(GetLauncherStatus(playerid))
		{
        	cef_emit_event(playerid, "data:pool:speedometer", CEFINT(1));
        	ShowSpeedometr_PTDLauncher(playerid);
		}
		else
		{
			ShowSpeedometr_PTD(playerid);
		}
		for(new m = 1; m <= TotalMansion; m++)
		{
		    for(new c; c != 7; c++)
	        if(newcar == MansionInfo[m][mCarStatus][c])
	        {
	            if(PI[playerid][pFamilyMansion] != MansionInfo[m][mID])
		     	{
	        		format(other_string, sizeof(other_string), "{3399FF}[%s - %s]{FFFFFF} | Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!", GetFamilyType(MansionInfo[m][mType]), MansionInfo[m][mName]);
	        		SendClientMessage(playerid, COLOR_WHITE, other_string);
					RemovePlayerFromVehicle(playerid);
					break;
				}
			}
		}
		for(new i; i < VehicleTotal; i++)
		{
		    if(newcar == FracCarID[i])
		    {
		    	if(PI[playerid][pMember] != FracID[i])
		        {
		        	format(other_string, sizeof(other_string), "{FFFFFF}Fraction: {3399FF}%s{FFFFFF} | Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!", GetFracName(FracID[i]));
		        	SendClientMessage(playerid, COLOR_WHITE, other_string);
					RemovePlayerFromVehicle(playerid);
					break;
				}
				else if(PI[playerid][pMember] == 3 && PI[playerid][pMember] == FracID[i])
				{
				    if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 433 && MaterialsStatus == false && MaterialsCount == 0)
				    {
				        SetPVarInt(playerid, "MaterialsStatus", 1);
				        DisablePlayerCheckpoint(playerid);
    					SetPlayerCheckpoint(playerid, 2759.0693, -2365.8198, 14.0694, 2.0);
					}
					else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 433 && MaterialsStatus == true && MaterialsCount < 500)
					{
					   	SetVehiclePos(GetPlayerVehicleID(playerid), 2759.0693,-2365.8198,14.0694);
    					SetVehicleZAngle(GetPlayerVehicleID(playerid), 0.7654);
					    errortxt(playerid, "Materialebis Srulad Chatvirtvamde Transportshi Chajdoma Sheudzlebelia!");
						RemovePlayerFromVehicle(playerid);
					}
				}
			}
		}
		for(new i; i < TotalCar; i++)
		{
		    if(newcar == CarInfo[i][CarIDInfo])
		    {
		        format(a_string, sizeof(a_string), "[AntiCheat]{FFFFFF} Motamashe: %s[%d] Shesadzloa Aketebs Teleports Mankanit", Name(playerid), playerid);
	    		SendAdminMessage(COLOR_WARNING, a_string);
	    		RemovePlayerFromVehicle(playerid);
	    		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		    }
		}
		if(newcar >= CleanCar[0] && newcar <= CleanCar[6])
		{
			if(Job[playerid] == 2)
			{
			    VehicleFuel[GetPlayerVehicleID(playerid)] = 30;
			    GetVehicleParamsEx(GetPlayerVehicleID(playerid), engine, lights, alarm, doors, bonnet, boot, objective);
			    SetVehicleParamsEx(GetPlayerVehicleID(playerid), VEHICLE_PARAMS_OFF, lights, alarm, 1, bonnet, boot, objective);
			    SetPlayerRaceCheckpoint(playerid, 0, CleaningPlace[0][0], CleaningPlace[0][1], CleaningPlace[0][2], CleaningPlace[1+1][0], CleaningPlace[1+1][1], CleaningPlace[1][2], 1.0);
			}
			else
			{
				errortxt(playerid, "Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!");
				RemovePlayerFromVehicle(playerid);
			}
		}
		if(newcar >= MechanicCar[0] && newcar <= MechanicCar[5])
		{
			if(Job[playerid] != 3)
			{
				errortxt(playerid, "Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!");
				RemovePlayerFromVehicle(playerid);
			}
		}
		if(newcar >= TaxiCar[0] && newcar <= TaxiCar[12])
		{
			if(Job[playerid] != 4)
			{
				errortxt(playerid, "Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!");
				RemovePlayerFromVehicle(playerid);
			}
			else if(Job[playerid] == 4)
			{
			    TaxiText[GetPlayerVehicleID(playerid)] = Text3D:INVALID_3DTEXT_ID;
			}
		}
		if(newcar >= MatCarrierCar[0] && newcar <= MatCarrierCar[6])
		{
			if(Job[playerid] != 6)
			{
				errortxt(playerid, "Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!");
				RemovePlayerFromVehicle(playerid);
			}
			else if(Job[playerid] == 6)
			{
			    SetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,1,bonnet,boot,objective);
                SetPVarInt(playerid, "MatCarrierCode", 1);
                SetPlayerCheckpoint(playerid, 844.8834,-599.4453,18.5456, 2);
                SendClientMessage(playerid, COLOR_WHITE, "{3399FF}Materialebis Gadamzidi{FFFFFF} | Materialebis Chasatvirti AdgilMdebareoba Monishnulia {3399FF}GPS{FFFFFF}'ze");
				SendClientMessage(playerid, COLOR_WHITE, "{3399FF}Materialebis Gadamzidi{FFFFFF} | Mushaobis Dasrulebistvis Sachiroa Transportidan Gadmosvla");
			}
		}
		if(newcar >= SchoolCar[0] && newcar <= SchoolCar[5])
		{
			if(PracticeStatus[playerid] == false)
			{
				errortxt(playerid, "Tkven Ar Gakvt Am Transportis Gamoyenebis Ufleba!");
				RemovePlayerFromVehicle(playerid);
			}
			else if(PracticeStatus[playerid] == true)
			{
				ShowPlayerDialog(playerid, INVALID_DIALOG_ID, DIALOG_STYLE_MSGBOX, "{3399FF}AutoSchool{FFFFFF} | Practice", "{FFFFFF}Praktikul Gamocdaze Maxsimaluri Sichkare Aris: {3399FF}30{FFFFFF}KM/H\n{FFFFFF}Gamocdaze Tkven Genichebat: {3399FF}3{FFFFFF}Shecdomis Ufleba\n{FFFFFF}Warmatebebs Gisurvebt, Shegidzliat Daiwyot Modzraoba!", "Daxurva", "");
                CheckpointCount[playerid] = 1;
				SetPlayerRaceCheckpoint(playerid, 2, -2034.9344,-140.6949,35.0572, -2034.9344,-140.6949,35.0572, 2.0);
			}
		}
	}
	else if(oldstate == 2) if((GetTickCount() - GetPVarInt(playerid,"AC_MCS")) <= 250 && PI[playerid][pAdmin] <= 0) return SendClientMessage(playerid,COLOR_RED,"Tqven Gaikicket Cheterobis Gamo"), Kick(playerid);
	if((newstate == 2 && oldstate == 3) || (newstate == 3 && oldstate == 2) && PI[playerid][pAdmin] <= 0) return SendClientMessage(playerid,COLOR_RED,"Tqven Gaikicket Cheterobis Gamo"), Kick(playerid);
	else if(newstate == PLAYER_STATE_ONFOOT)
	{
	    DisablePlayerCheckpoint(playerid), SetPVarInt(playerid, "GPSCode", 0), SetPVarInt(playerid, "MaterialsStatus", 0);
	    if(Job[playerid] == 6 && GetPVarInt(playerid, "MatCarrierCode") != 0)
	    {
	        format(other_string, sizeof(other_string), "{3399FF}Materialebis Gadamzidi{FFFFFF} | Tkven Daasrulet Samushao Dge. Dagericxat: {3399FF}%d$", MatJobMoney[playerid]);
	        SendClientMessage(playerid, COLOR_WHITE, other_string);
	        new money_str[15];
		    format(money_str, sizeof(money_str), "~g~+%d$", MatJobMoney[playerid]);
		    GameTextForPlayer(playerid, money_str, 2000, 1);
			PI[playerid][pMoney] += MatJobMoney[playerid];
			UpdatePlayerData(playerid, "Money", PI[playerid][pMoney]);
			format(log_str, sizeof(log_str), "[Materialebis Gadamzidi] %s'm(a) Daasrula Samushao Da Gamoimushava: %d$", Name(playerid), MatJobMoney[playerid]);
			CreateLog("Job", log_str);
			MatJobMoney[playerid] = 0;
			MatCarrierCount[playerid] = 0;
	        SetTimerEx("MatCarrierCarRespawn", 2000, false, "d", playerid);
			SetPVarInt(playerid, "MatCarrierCode", 0);
			DisablePlayerCheckpoint(playerid);
	    }
   		if(GetLauncherStatus(playerid))
		{
        	cef_emit_event(playerid, "data:pool:speedometer", CEFINT(0));
		}
		else
		{
			HideSpeedometr_PTD(playerid);
		}
	}
	if(newstate == PLAYER_STATE_PASSENGER)
	{
		foreach(new i:Player)
		{
			if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid)) && GetPlayerState(i) == 2 && TransportDuty[i] == 1)
			{
				if(GetPVarInt(i,"Taxi_") == 0)
				{
					format(other_string, sizeof(other_string), "Player - {3399FF}%s{FFFFFF} | Chajda Tkvens Transportshi, Miiyvanet Danishnulebis Adgilas", Name(playerid));
					SendClientMessage(i, COLOR_WHITE, other_string);
					SetPVarInt(i,"Taxi_",playerid+1);
				}
				break;
			}
		}
	}
	return true;
