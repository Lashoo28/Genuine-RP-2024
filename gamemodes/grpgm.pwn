	if(PI[playerid][pFracStatus] == 0) return errortxt(playerid, "Tkven Ar Gacviat Samushao Tansacmeli!");
	if(CopGethere[playerid] != INVALID_PLAYER_ID) return errortxt(playerid, "Tkven Ukve Miatrevt Motamashes!");
	if(IsPlayerInAnyVehicle(playerid)) return errortxt(playerid, "Tkven Imyofebit Transportshi!");
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /gotome [Motamashis ID]");
	if(params[0] == INVALID_PLAYER_ID) return errortxt(playerid, "Motamashe Ver Moidzebna!");
	if(PI[params[0]][pLogin] == 0) return errortxt(playerid, "Motamashes Jer Ar Gauvlia Avtorizacia!");
	if(IsPlayerInAnyVehicle(params[0])) return errortxt(playerid, "Motamashe Imyofeba Transportshi!");
	if(!ProxDetectorS(2.0, playerid, params[0])) return errortxt(playerid, "Motamashe Ar Imyofeba Tkventan Axlos!");
	if(PI[params[0]][pWanted] < 1) return errortxt(playerid, "Motamashes Ar Adevs Dzebnis Done!");
	if(Cuffed[params[0]] == false) return errortxt(playerid, "Motamashes Ar Adevs Borkilebi!");
	if(params[0] == playerid) return errortxt(playerid, "Tkven Miutitet Tkveni ID!");
	if(IsACop(params[0])) return infotxt(playerid, "Tkven Miutitet SamartalDamcavis ID!");
	if(GethereStatus[params[0]] == true) return errortxt(playerid, "Motamashes Miatrevs SamartalDamcavi!");
	if(PI[params[0]][pJail] != 0) return errortxt(playerid, "Motamashe Imyofeba Cixeshi");
	CopStatus[playerid] = true;
    GethereStatus[params[0]] = true;
    PlayerGethere[params[0]] = playerid;
	CopGethere[playerid] = params[0];
    new string[180];
    format(string,sizeof(string),"Officer: %s Miatrevs Damnashaves %s's", Name(playerid), Name(params[0]));
	ProxDetector(20.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	return true;
}
