		else if(PI[playerid][pAdmin] == 9)
		{
		    SetPlayerChatBubble(playerid,"« Server Manager »",COLOR_DUTY,30.0,1000);
		}
		else if(strcmp("Aleksandre_Kinkladze",name,false) == 0)
		{
		    SetPlayerChatBubble(playerid,"« Server Owner »",COLOR_DUTY,30.0,1000);
		}
		else
		{
		    SetPlayerChatBubble(playerid,"« Spec Administrator »",COLOR_DUTY,30.0,1000);
		}
    }
    //CASINOMAGIDEBI
    new stolid = GetPVarInt(playerid,"GameStol");
