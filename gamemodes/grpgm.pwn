					McInfo[i][FrakVlad] = McInfo[i][Napad];
					McInfo[i][Napad] = 0;
				}
    			ZoneOnMBattle[i] = 0;
				GangZoneStopFlashForAll(McInfo[i][ID]);
				GangZoneHideForAll(McInfo[i][ID]);
