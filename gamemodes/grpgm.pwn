public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
	if(PI[playerid][pMember] == 1 && PI[playerid][pFracStatus] == 1)
	{
	    if(CopStatus[playerid] == true)
	    {
	        ClearAnimations(playerid);
	        errortxt(playerid, "Tkven Miatrevt Damnashaves Da Ver Chajdebit Transportshi!");
	        return true;
	    }
	}
	if(!ispassenger)
	{
 		new newcar = GetPlayerVehicleID(playerid);
	    if(PI[playerid][pLicenses][0] == 0 && PracticeStatus[playerid] == false)
	    {
			if(VehicleLic(newcar))
			{
			    SendClientMessage(playerid, COLOR_WHITE, "{3399FF}Martvis Mowmoba{FFFFFF} | Tkven Ar Flobt Am Transportis Martvis Uflebas!");
			    RemovePlayerFromVehicle(playerid);
			}
	    }
		if(VehID[playerid][0] != vehicleid && VehID[playerid][1] != vehicleid && VehID[playerid][2] != vehicleid)
		{
			new vehowner = GetVehicleOwner(vehicleid);
			if(vehowner != INVALID_PLAYER_ID)
			{
			    if(GetPVarInt(playerid, "KeyFlood") < gettime())
			    {
					SetPVarInt(playerid, "KeyFlood", gettime()+3);
					new exQuery[350];
					mysql_format(connect, exQuery, 350, "SELECT `Owner`,`Player` FROM `carkeys` WHERE `Owner` = '%s' AND `Player` = '%s'",Name(vehowner),Name(playerid));
					mysql_tquery(connect, exQuery, "SearchForKeys", "ii", playerid, vehowner);
				}
				else
				{
				    ClearAnimations(playerid);
					new Float:X, Float:Y, Float:Z;
					GetPlayerPos(playerid, X, Y, Z);
					SetPlayerPos(playerid, X, Y, Z);
				}
			}
		}
	}
	return true;
}
public OnPlayerExitVehicle(playerid, vehicleid)
{
    SetPVarInt(playerid,"AFK_Time", 0);
	if(PracticeStatus[playerid] == true)
	{
	    PracticeStatus[playerid] = false;
	    CheckpointCount[playerid] = 0;
	    DisablePlayerRaceCheckpoint(playerid);
	    SetVehicleToRespawn(GetPlayerVehicleID(playerid));
	}

	if(Job[playerid] == 2)
	{
	    Job[playerid] = 0, CleaningCheckpoint[playerid] = 0;
		CleaningMoney[playerid] = 0, DisablePlayerRaceCheckpoint(playerid);

	    GetVehicleParamsEx(GetPlayerVehicleID(playerid), engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(GetPlayerVehicleID(playerid), VEHICLE_PARAMS_OFF, lights, alarm, 0, bonnet, boot, objective);
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));

		SetPlayerSkinEx(playerid);
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && TransportDuty[playerid] == 1)
    {
		TransportDuty[playerid] = 0;
		if(TaxiText[GetPlayerVehicleID(playerid)] != Text3D:INVALID_3DTEXT_ID)
		{
			Delete3DTextLabel(TaxiText[GetPlayerVehicleID(playerid)]);
			TaxiText[GetPlayerVehicleID(playerid)] = Text3D:INVALID_3DTEXT_ID;
